a:6:{s:7:"headers";a:8:{i:0;s:27:"X-Frame-Options: SAMEORIGIN";i:1;s:31:"X-Content-Type-Options: nosniff";i:2;s:31:"X-XSS-Protection: 1; mode=block";i:3;s:36:"Content-Type: application/javascript";i:4;s:13:"X-Cache: MISS";i:5;s:31:"X-Cache-Instance: wp-brutus-cli";i:6;s:27:"X-Powered-By: Ikabud-Kernel";i:7;s:35:"Cache-Control: public, max-age=3600";}s:4:"body";s:956:"jQuery(document).ready(function ($) {
    var header = document.getElementById("main-navigation-bar");
    var masthead = document.getElementById("masthead");

    if (!header || !masthead) return;

    var stickyHeight = header.offsetHeight;
    var sticky = header.offsetTop + stickyHeight;
    var isStickyActive = false;

    function applyStickyHeader() {
        if (window.pageYOffset > sticky) {
            if (!isStickyActive) {
                header.classList.add("aft-sticky-navigation");
                masthead.style.paddingBottom = stickyHeight + "px";
                isStickyActive = true;
            }
        } else {
            if (isStickyActive) {
                header.classList.remove("aft-sticky-navigation");
                masthead.style.paddingBottom = "0px";
                isStickyActive = false;
            }
        }
    }

    $(window).on('scroll', applyStickyHeader);
    applyStickyHeader(); // Run on load
});
