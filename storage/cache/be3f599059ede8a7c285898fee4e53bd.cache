a:6:{s:7:"headers";a:7:{i:0;s:27:"X-Frame-Options: SAMEORIGIN";i:1;s:31:"X-Content-Type-Options: nosniff";i:2;s:31:"X-XSS-Protection: 1; mode=block";i:3;s:341:"Content-Security-Policy: default-src 'self' https: http:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https: http: blob:; style-src 'self' 'unsafe-inline' https: http:; img-src 'self' data: https: http:; font-src 'self' data: https: http:; connect-src 'self' https: http: wss: ws:; frame-src 'self' https: http:; worker-src 'self' blob:;";i:4;s:48:"Referrer-Policy: strict-origin-when-cross-origin";i:5;s:60:"Permissions-Policy: geolocation=(), microphone=(), camera=()";i:6;s:36:"Content-type: text/css;charset=UTF-8";}s:4:"body";s:9008:"/*--------------------------------------------------------------
# Enhanced Homepage Styles
--------------------------------------------------------------*/

/*--------------------------------------------------------------
## Featured Slider / Hero Section
--------------------------------------------------------------*/
.hero-section {
    background: #000;
    position: relative;
    overflow: hidden;
}

.featured-posts-slider {
    position: relative;
    height: 600px;
}

.featured-slide {
    position: relative;
    height: 100%;
    display: flex;
    align-items: center;
}

.slide-image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
}

.slide-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.slide-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(to right, rgba(0, 0, 0, 0.8) 0%, rgba(0, 0, 0, 0.4) 50%, transparent 100%);
    z-index: 2;
}

.slide-content {
    position: relative;
    z-index: 3;
    max-width: 600px;
    padding: 2rem;
}

.slide-category {
    display: inline-block;
    padding: 0.5rem 1rem;
    background: #667eea;
    color: #fff;
    font-size: 0.875rem;
    font-weight: 600;
    border-radius: 4px;
    margin-bottom: 1rem;
}

.slide-content h2 {
    margin: 0 0 1rem;
}

.slide-content a {
    color: #fff;
    text-decoration: none;
}

.slide-content a:hover {
    color: #667eea;
}

.slide-meta {
    display: flex;
    gap: 0.5rem;
    align-items: center;
    margin: 1rem 0;
}

.slide-meta .separator {
    color: rgba(255, 255, 255, 0.5);
}

.read-more-btn {
    display: inline-block;
    padding: 0.75rem 2rem;
    background: #667eea;
    color: #fff;
    text-decoration: none;
    border-radius: 4px;
    font-weight: 600;
    margin-top: 1rem;
    transition: all 0.3s ease;
}

.read-more-btn:hover {
    background: #5568d3;
    transform: translateY(-2px);
    box-shadow: 0 4px 12px rgba(102, 126, 234, 0.4);
}

/*--------------------------------------------------------------
## Section Headers
--------------------------------------------------------------*/
.section-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 2rem;
    padding-bottom: 1rem;
    border-bottom: 2px solid #667eea;
}

.section-header h2 {
    margin: 0;
}

.view-all {
    color: #667eea;
    text-decoration: none;
    font-weight: 500;
    transition: color 0.3s ease;
}

.view-all:hover {
    color: #5568d3;
}

/*--------------------------------------------------------------
## Trending Posts
--------------------------------------------------------------*/
.trending-posts-grid {
    margin-bottom: 3rem;
}

.trending-post-card {
    border: 1px solid #e9ecef;
    border-radius: 8px;
    overflow: hidden;
    transition: all 0.3s ease;
}

.trending-post-card:hover {
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.1);
    transform: translateY(-4px);
}

.trending-post-card .post-thumbnail {
    position: relative;
    height: 200px;
    overflow: hidden;
}

.trending-post-card .post-thumbnail img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}

.trending-post-card:hover .post-thumbnail img {
    transform: scale(1.05);
}

.post-category {
    position: absolute;
    top: 1rem;
    left: 1rem;
    padding: 0.25rem 0.75rem;
    background: #667eea;
    color: #fff;
    font-size: 0.75rem;
    font-weight: 600;
    border-radius: 4px;
    z-index: 2;
}

.trending-post-card .post-content {
    padding: 1.5rem;
}

.trending-post-card h3 {
    margin: 0 0 0.5rem;
}

.trending-post-card h3 a {
    color: #1a1a1a;
    text-decoration: none;
}

.trending-post-card h3 a:hover {
    color: #667eea;
}

.post-meta {
    display: flex;
    gap: 0.5rem;
    align-items: center;
    margin: 0.75rem 0;
}

.post-meta .separator {
    color: #999;
}

.author-avatar-small {
    width: 24px;
    height: 24px;
    border-radius: 50%;
    margin-right: 0.25rem;
    vertical-align: middle;
}

/*--------------------------------------------------------------
## Latest Posts
--------------------------------------------------------------*/
.latest-posts-list {
    margin-bottom: 3rem;
}

.latest-post-card {
    border: 1px solid #e9ecef;
    border-radius: 8px;
    overflow: hidden;
    transition: all 0.3s ease;
}

.latest-post-card:hover {
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.1);
}

.post-layout {
    display: grid;
    grid-template-columns: 300px 1fr;
    gap: 2rem;
}

.post-thumbnail-large {
    position: relative;
    height: 200px;
    overflow: hidden;
}

.post-thumbnail-large img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
}

.latest-post-card:hover .post-thumbnail-large img {
    transform: scale(1.05);
}

.post-content-large {
    padding: 1.5rem 1.5rem 1.5rem 0;
}

.post-content-large h2 {
    margin: 0 0 0.75rem;
}

.post-content-large h2 a {
    color: #1a1a1a;
    text-decoration: none;
}

.post-content-large h2 a:hover {
    color: #667eea;
}

.read-more-link {
    display: inline-block;
    color: #667eea;
    text-decoration: none;
    font-weight: 500;
    margin-top: 1rem;
    transition: color 0.3s ease;
}

.read-more-link:hover {
    color: #5568d3;
}

/*--------------------------------------------------------------
## Pagination
--------------------------------------------------------------*/
.pagination-wrapper {
    margin-top: 3rem;
}

.pagination {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 1rem;
}

.page-link {
    padding: 0.75rem 1.5rem;
    background: #fff;
    border: 1px solid #e9ecef;
    border-radius: 4px;
    color: #667eea;
    text-decoration: none;
    font-weight: 500;
    transition: all 0.3s ease;
}

.page-link:hover {
    background: #667eea;
    color: #fff;
    border-color: #667eea;
}

.page-numbers {
    color: #666;
    font-weight: 500;
}

/*--------------------------------------------------------------
## Category Showcase
--------------------------------------------------------------*/
.category-card {
    text-align: center;
    padding: 2rem;
    border: 1px solid #e9ecef;
    border-radius: 8px;
    transition: all 0.3s ease;
}

.category-card:hover {
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.1);
    transform: translateY(-4px);
    border-color: #667eea;
}

.category-icon {
    width: 80px;
    height: 80px;
    margin: 0 auto 1rem;
    border-radius: 50%;
    background: #f8f9fa;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 2rem;
}

.category-icon img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 50%;
}

.category-card h3 {
    margin: 0 0 0.5rem;
}

.category-card h3 a {
    color: #1a1a1a;
    text-decoration: none;
}

.category-card:hover h3 a {
    color: #667eea;
}

/*--------------------------------------------------------------
## Newsletter Section
--------------------------------------------------------------*/
.newsletter-box {
    text-align: center;
    padding: 2rem;
}

.newsletter-form-large {
    max-width: 600px;
    margin: 2rem auto 0;
}

.newsletter-form-large .form-group {
    display: flex;
    gap: 0.5rem;
}

.newsletter-input {
    flex: 1;
    padding: 1rem 1.5rem;
    border: 2px solid rgba(255, 255, 255, 0.3);
    border-radius: 4px;
    background: rgba(255, 255, 255, 0.1);
    color: #fff;
    font-size: 1rem;
}

.newsletter-input::placeholder {
    color: rgba(255, 255, 255, 0.7);
}

.newsletter-button {
    padding: 1rem 2rem;
    background: #fff;
    color: #667eea;
    border: none;
    border-radius: 4px;
    font-weight: 600;
    font-size: 1rem;
    cursor: pointer;
    transition: all 0.3s ease;
}

.newsletter-button:hover {
    background: #f8f9fa;
    transform: translateY(-2px);
    box-shadow: 0 4px 12px rgba(255, 255, 255, 0.3);
}

.newsletter-privacy {
    margin-top: 1rem;
    font-size: 0.875rem;
    color: rgba(255, 255, 255, 0.8);
}

/*--------------------------------------------------------------
## Responsive
--------------------------------------------------------------*/
@media (max-width: 1024px) {
    .post-layout {
        grid-template-columns: 250px 1fr;
    }
}

@media (max-width: 768px) {
    .featured-posts-slider {
        height: 400px;
    }
    
    .slide-content {
        max-width: 100%;
    }
    
    .section-header {
        flex-direction: column;
        align-items: flex-start;
        gap: 0.5rem;
    }
    
    .post-layout {
        grid-template-columns: 1fr;
    }
    
    .post-thumbnail-large {
        height: 250px;
    }
    
    .post-content-large {
        padding: 1.5rem;
    }
    
    .newsletter-form-large .form-group {
        flex-direction: column;
    }
}

@media (max-width: 480px) {
    .featured-posts-slider {
        height: 300px;
    }
    
    .slide-content {
        padding: 1rem;
    }
}
